<SvgComponent @ref="svgref" Width="100" Height="100" />

@code
{
    [Parameter]
    public SvgValue ViewBoxHeight { get; set; }

    [Parameter]
    public SvgValue ViewBoxWidth { get; set; }

    private SvgComponent svgref;

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            var circle = new SvgCircle
            {
                CenterX = 50,
                CenterY = 50,
                Radius = 25,
            };

            svgref.Add(circle);
        }
    }
}